unit:GM-PER-NUM
    rdf:type qudt:Unit ;
    dcterms:description "Grams per number - average mass per discrete object" ;
    qudt:plainTextDescription "A unit for expressing average mass per discrete object, used in ecological studies for individual organism mass, particle characterization, and size distribution analysis" ;
    qudt:applicableSystem sou:SI ;
    qudt:hasDimensionVector qkdv:A0E0L0I0M1H0T0D0 ;
    qudt:hasQuantityKind quantitykind:Mass ;
    qudt:conversionMultiplier 0.001 ;
    qudt:conversionMultiplierSN 1.0E-3 ;
    qudt:symbol "g·N⁻¹" ;
    rdfs:isDefinedBy <http://qudt.org/2.1/vocab/unit> ;
    rdfs:label "Gram per Number"@en .

unit:GigaGM
    rdf:type qudt:Unit ;
    dcterms:description "Gigagrams - very large mass unit equivalent to kilotons" ;
    qudt:plainTextDescription "A unit of very large mass equal to one billion grams or one thousand metric tons, used in large-scale environmental assessments, carbon budgets, and industrial measurements" ;
    qudt:applicableSystem sou:SI ;
    qudt:hasDimensionVector qkdv:A0E0L0I0M1H0T0D0 ;
    qudt:hasQuantityKind quantitykind:Mass ;
    qudt:conversionMultiplier 1000000 ;
    qudt:conversionMultiplierSN 1.0E6 ;
    qudt:symbol "Gg" ;
    rdfs:isDefinedBy <http://qudt.org/2.1/vocab/unit> ;
    rdfs:label "Gigagram"@en .

unit:MI_US-PER-SEC
    rdf:type qudt:Unit ;
    dcterms:description "US miles per second - high velocity in US customary units" ;
    qudt:plainTextDescription "A unit of high velocity in the US customary system, used in ballistics, meteorology, and high-speed transport studies" ;
    qudt:applicableSystem sou:USCS ;
    qudt:hasDimensionVector qkdv:A0E0L1I0M0H0T-1D0 ;
    qudt:hasQuantityKind quantitykind:Velocity ;
    qudt:conversionMultiplier 1609.344 ;
    qudt:conversionMultiplierSN 1.609344E3 ;
    qudt:symbol "mi·s⁻¹" ;
    rdfs:isDefinedBy <http://qudt.org/2.1/vocab/unit> ;
    rdfs:label "US Mile per Second"@en .

unit:M3-PER-M2-DAY
    rdf:type qudt:Unit ;
    dcterms:description "Cubic meters per square meter per day - daily volume flux per unit area" ;
    qudt:plainTextDescription "A unit for measuring daily volume fluxes per unit area, effectively representing daily depth accumulation. Used in hydrology, precipitation studies, and water balance calculations" ;
    qudt:applicableSystem sou:SI ;
    qudt:hasDimensionVector qkdv:A0E0L1I0M0H0T-1D0 ;
    qudt:hasQuantityKind quantitykind:Velocity ;
    qudt:conversionMultiplier 0.00001157407407407407407407407407407407 ;
    qudt:conversionMultiplierSN 1.157407407407407407407407407407407E-5 ;
    qudt:symbol "m³·m⁻²·d⁻¹" ;
    rdfs:isDefinedBy <http://qudt.org/2.1/vocab/unit> ;
    rdfs:label "Cubic Meter per Square Meter Day"@en .

