    qudt:hasDimensionVector qkdv:A0E0L1I0M0H0T0D0 ;
    qudt:hasQuantityKind quantitykind:VolumePerArea ;
    qudt:conversionMultiplier 0.0008956473829201101 ;
    qudt:conversionMultiplierSN 8.956473829201101E-4 ;
    qudt:symbol "cord/ac" ;
    rdf:type qudt:DerivedUnit ;
    rdfs:isDefinedBy <http://qudt.org/2.1/vocab/unit> .

